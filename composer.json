{
    "name": "abdul-quadri/laravel-activity-logger",
    "description": "A comprehensive activity logging package for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Quadri",
            "email": "abdulquadri.aq@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "AbdulQuadri\\ActivityLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AbdulQuadri\\ActivityLogger\\ActivityLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}